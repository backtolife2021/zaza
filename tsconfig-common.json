{
  // @see https://typescripttolua.github.io/docs/configuration
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/vscode-typescript-to-lua/master/tsconfig-schema.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "noImplicitThis": true,
    "target": "esnext",
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node"
  }
}
